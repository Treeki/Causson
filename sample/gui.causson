type Wind = record {
	gui:Window wnd
	gui:Box boxxo
	gui:Button butt1
	gui:Button butt2
}

component CompWind {
	wnd = gui:Window {
		gui:Box(gui:Orientation:Vertical) {
			.spacing = 10
			butt1 = gui:Button { .label = "Butt1" }
			butt2 = gui:Button { .label = "Butt2" }
		}
	}
}

def main() -> void {
	gui:init()
	let wind = CompWind:new()
	wind.wnd.show()
	gui:run()
}

